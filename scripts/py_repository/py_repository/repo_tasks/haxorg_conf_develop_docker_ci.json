{
  "log_level"          : "NORMAL",
  "use_unchanged_tasks": true,
  "instrument"         : {"coverage": true},
  "build_conf"         : {"cmake_generator": "Ninja"},
  "develop_ci_conf"    : {
    "emscripten_deps" : false,
    "emscripten_build": false,
    "emscripten_test" : false,
    "reflection"      : false,
    "coverage"        : true,
    "docs"            : false,
    "include_graph"   : false,
    "symbol_size"     : false
  },
  "coverage_conf"      : {
    "coverage_run_whitelist" : [ {"class_pattern": "OrgParseSem"} ],
    "coverage_html_whitelist": [ "OrgParser"                      ]
  }
}
